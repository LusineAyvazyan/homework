	<title>xndir6</title>
	<script type="text/javascript">
		let num = +prompt("insert number");
		let a = 0;
		let b = 1;
		 for (let i = num; i >= (a + b); i--) {
		        c = a + b;	 	
		 	 	a = b;
		        b = c;
		        document.write(c + "<br>");
		 	}
	</script>

